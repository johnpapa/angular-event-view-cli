{
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "event-view-cli": {
      "root": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/api",
                "output": "/api"
              }
            ],
            "styles": [
              {
                "input": "src/assets/material.min.css",
                "inject": true
              },
              {
                "input": "src/assets/sprite-av-white.css",
                "inject": true
              },
              {
                "input": "src/assets/animate.css",
                "inject": true
              },
              {
                "input": "src/assets/app.css",
                "inject": true
              },
              {
                "input": "src/styles.css",
                "inject": true
              }
            ],
            "scripts": [
              {
                "input": "src/assets/material.min.js",
                "inject": true
              }
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "src": "src/environments/environment.ts",
                  "replaceWith": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "event-view-cli:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "event-view-cli:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "event-view-cli:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              {
                "input": "src/assets/material.min.js",
                "inject": true
              }
            ],
            "styles": [
              {
                "input": "src/assets/material.min.css",
                "inject": true
              },
              {
                "input": "src/assets/sprite-av-white.css",
                "inject": true
              },
              {
                "input": "src/assets/animate.css",
                "inject": true
              },
              {
                "input": "src/assets/app.css",
                "inject": true
              },
              {
                "input": "src/styles.css",
                "inject": true
              }
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/api",
                "output": "/api"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "event-view-cli-e2e": {
      "root": "",
      "projectType": "application",
      "cli": {},
      "schematics": {},
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "event-view-cli:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "ev",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "ev"
    }
  }
}